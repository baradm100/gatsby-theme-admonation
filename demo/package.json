{
    "private": true,
    "name": "demo",
    "version": "0.1.0",
    "license": "MIT",
    "scripts": {
        "build": "gatsby build",
        "develop": "gatsby develop",
        "format": "prettier --write src/**/*.{js,jsx}",
        "start": "npm run develop",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "update:theme": "cd ../theme && npm run pack:local && cd ../demo && npm i --save ../theme/output.tgz",
        "update:theme:clean": "npm uninstall @baradm100/gatsby-theme-admonation && npm run update:theme",
        "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
    },
    "dependencies": {
        "@baradm100/gatsby-theme-admonation": "file:../theme/output.tgz",
        "gatsby": "^2.13.1",
        "react": "^16.8.6",
        "react-carbon": "^1.0.7",
        "react-dom": "^16.8.6"
    },
    "devDependencies": {
        "prettier": "^1.18.2"
    }
}
